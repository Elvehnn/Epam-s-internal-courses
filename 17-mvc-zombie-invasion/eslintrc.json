{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"extends": [
		"eslint:recommended",
		"prettier"
	],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"import/extensions": 0,
		"import/prefer-default-export": "off",
		"linebreak-style": 0
	}
}
